{
  "version": 2,
  "builds": [
    {
      "src": "privacy-policy.html",
      "use": "@vercel/static"
    },
    {
      "src": "terms-conditions.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/privacy",
      "destination": "/privacy-policy.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms-conditions.html"
    },
    {
      "source": "/terms-conditions",
      "destination": "/terms-conditions.html"
    },
    {
      "source": "/terms-of-service",
      "destination": "/terms-conditions.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
